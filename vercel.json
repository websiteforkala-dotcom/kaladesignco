{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/admin",
            "destination": "/kdc-admin",
            "permanent": true
        },
        {
            "source": "/admin/:match*",
            "destination": "/kdc-admin/:match*",
            "permanent": true
        },
        {
            "source": "/login",
            "destination": "/kdc-admin/login",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}